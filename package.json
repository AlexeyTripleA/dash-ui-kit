{
  "name": "dash-ui",
  "version": "0.1.0",
  "description": "Dash UI Kit for React js",
  "keywords": [
    "dash",
    "ui-kit",
    "reactjs",
    "react",
    "tailwind",
    "cva"
  ],
  "license": "ISC",
  "type": "module",
  "author": "pshenmic.dev",
  "files": [
    "dist"
  ],
  "exports": {
    "./react": {
      "types": "./dist/react/index.d.ts",
      "browser": "./dist/react/index.esm.js",
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.cjs.js",
      "default": "./dist/react/index.cjs.js"
    },
    "./styles": "./dist/styles.css",
    "./theme": "./dist/theme.css",
    "./react/*": {
      "types": "./dist/react/*/index.d.ts",
      "browser": "./dist/react/*/index.esm.js",
      "import": "./dist/react/*/index.esm.js",
      "require": "./dist/react/*/index.cjs.js",
      "default": "./dist/react/*/index.cjs.js"
    }
  },
  "scripts": {
    "types": "tsc -p tsconfig.build.json",
    "build": "rollup -c && npm run types",
    "prepare": "rollup -c && npm run types",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.5",
    "class-variance-authority": "^0.7.1",
    "minidenticons": "^4.2.1",
    "radix-ui": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/eslintrc": "^3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/experimental-nextjs-vite": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-storybook": "^0.12.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-simple-vars": "^7.0.1",
    "rollup": "^4.41.0",
    "rollup-plugin-multi-input": "^1.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.6.14",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "typesVersions": {
    "*": {
      "react": [
        "dist/react/index.d.ts"
      ],
      "*": [
        "dist/react/*/index.d.ts"
      ]
    }
  }
}
